{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {},
    "typecheck": {},
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}


{
  "name": "news-api",
  "version": "0.1.0",
  "private": true,
  "description": "RSS ingestion, enrichment, and API platform",
  "workspaces": [
    "apps/*",
    "packages/*",
    "supplier_capabilities"
  ],
  "packageManager": "npm@11.6.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "cleanup:dedupe-articles": "tsx scripts/cleanup/remove-duplicate-articles.ts",
    "search:backfill": "tsx scripts/search-backfill.ts",
    "test:story-maintenance": "tsx scripts/test-story-maintenance.ts",
    "test:runtime-story": "tsx scripts/test-runtime-story-maintenance.ts",
    "verify:story-setup": "tsx scripts/verify-story-setup.ts",
    "benchmark:search": "tsx scripts/benchmark-search.ts"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "prisma": "^6.19.0",
    "turbo": "^2.1.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "next-themes": "^0.3.0"
  }
}
